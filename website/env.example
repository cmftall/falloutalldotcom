# Analytics Configuration
NEXT_PUBLIC_GA_TRACKING_ID=G-XXXXXXXXXX
NEXT_PUBLIC_PLAUSIBLE_DOMAIN=yourdomain.com

# Google Search Console Verification
GOOGLE_VERIFICATION_CODE=your_verification_code

# Error Monitoring (Sentry)
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# Security Headers (set to 'true' for non-static deployments)
NEXT_PUBLIC_USE_HEADERS=false

# Site Configuration
NEXT_PUBLIC_SITE_URL=https://falloutall.com
NEXT_PUBLIC_SITE_NAME="Fallou TALL"

# Environment
NODE_ENV=production
